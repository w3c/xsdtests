        <schemaTest name="zone403.xsd">
            <schemaDocument xlink:href="../oracleData/Zone/zone403.xsd"/>
            <expected validity="valid"/>
            <current status="accepted" date="2011-02-24"/>
        </schemaTest>
        <instanceTest name="zone403.v1.xml">
		        <annotation>
		            <documentation>
		                valid, timezone present.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../oracleData/Zone/zone403.v1.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2011-02-24"/>
        </instanceTest>  
        <instanceTest name="zone403.v2.xml">
		        <annotation>
		            <documentation>
		                valid, timezone present.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../oracleData/Zone/zone403.v2.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2011-02-24"/>
        </instanceTest>                
        <instanceTest name="zone403.n1.xml">
		        <annotation>
		            <documentation>
		                Invalid, timezone not present.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../oracleData/Zone/zone403.n1.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2011-02-24"/>
        </instanceTest>                
    </testGroup>
    
