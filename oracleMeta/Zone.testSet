    <testGroup version="1.1" name="zone403">
        <annotation>
            <documentation>
                <Title>Test new timezone facet explicitTimezone, value="required" for datatype xs:dateTime</Title>
                <Description>Test new timezone facet explicitTimezone, value="required" for datatype xs:dateTime</Description>
            </documentation>
        </annotation>
