    <testGroup version="1.1" name="zone401">
        <annotation>
            <documentation>
                <Title>Equality testing (enumeration) for dateTime values, use YEAR as ZERO</Title>
                <Description>Use Year as Zero </Description>
            </documentation>
        </annotation>
        <documentationReference xlink:href="http://www.w3.org/TR/2009/WD-xmlschema11-2-20091203/datatypes.html#rf-explicitTimezone"/>
        <documentationReference xlink:href="../common/XSD1_1TestCategories.xml#xsd1_1-DateTimeTypes-ExplicitTZFacet"/>
        <schemaTest name="zone401.xsd">
            <schemaDocument xlink:href="../oracleData/Zone/zone401.xsd"/>
            <expected validity="valid"/>
            <current status="accepted" date="2011-02-24"/>
        </schemaTest>
        <instanceTest name="zone401.v1.xml">
		        <annotation>
		            <documentation>
		                Valid date per enumeration
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../oracleData/Zone/zone401.v1.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2011-02-24"/>
        </instanceTest>  
        <instanceTest name="zone401.v2.xml">
		        <annotation>
		            <documentation>
		                Valid date per enumeration
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../oracleData/Zone/zone401.v2.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2011-02-24"/>
        </instanceTest>                
        <instanceTest name="zone401.v3.xml">
		        <annotation>
		            <documentation>
		                Valid date per enumeration
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../oracleData/Zone/zone401.v3.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2011-02-24"/>
        </instanceTest>                
        <instanceTest name="zone401.n1.xml">
		        <annotation>
		            <documentation>
		                Invalid date per enumeration
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../oracleData/Zone/zone401.n1.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2011-02-24"/>
        </instanceTest>                      
        <instanceTest name="zone401.n2.xml">
		        <annotation>
		            <documentation>
		                Invalid date per enumeration
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../oracleData/Zone/zone401.n2.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2011-02-24"/>
        </instanceTest>                      
    </testGroup>    
    
