    <testGroup name="simple003">
        <annotation>
            <documentation>
                <Title>Test that simpleType/@final = extension is allowed</Title>
                <Description>Depends on resolution of spec bug 2074</Description>
            </documentation>
        </annotation>
        <documentationReference xlink:href="http://www.w3.org/TR/xmlschema-1/#d0e13819"/>
        <documentationReference xlink:href="https://www.w3.org/Bugs/Public/show_bug.cgi?id=2074"/>
        <schemaTest name="simple003.xsd">
            <schemaDocument xlink:href="../saxonData/Simple/simple003.xsd"/>
            <expected validity="valid"/>
            <current status="accepted" date="2012-04-25"/>
        </schemaTest>
        <instanceTest name="simple003.v01.xml">
		        <annotation>
		            <documentation>
		                Valid, the simpleType that prohibits extension is not extended.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Simple/simple003.v01.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2012-04-25"/>
        </instanceTest>         
