     <testGroup name="simple001">
        <annotation>
            <documentation>
                <Title>+INF allowed in xs:double lexical space</Title>
                <Description>+INF allowed in xs:double lexical space in XSD 1.1</Description>
            </documentation>
        </annotation>
        <documentationReference xlink:href="http://www.w3.org/TR/xmlschema-1/#d0e13819"/>
        <documentationReference xlink:href="../common/XSD1_1TestCategories.xml#xsd1_1-Misc-LexicalRepForFloatAndDouble"/>
        <schemaTest name="simple001.xsd">
            <schemaDocument xlink:href="../saxonData/Simple/simple001.xsd"/>
            <expected validity="valid"/>
            <current status="accepted" date="2010-10-08"/>
        </schemaTest>
        <instanceTest name="simple001.v01.xml">
		        <annotation>
		            <documentation>
		                Valid, +INF used legitimately.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Simple/simple001.v01.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2010-10-08"/>
        </instanceTest>        
        <instanceTest name="simple001.n01.xml">
		        <annotation>
		            <documentation>
		                Invalid, -INF conflicts with fixed value.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Simple/simple001.n01.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2010-10-08"/>
        </instanceTest>        
        <instanceTest name="simple001.n02.xml">
		        <annotation>
		            <documentation>
		                Invalid, NaN conflicts with fixed value.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Simple/simple001.n02.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2010-10-08"/>
        </instanceTest>               
    </testGroup>
    
