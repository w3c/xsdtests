        <documentationReference xlink:href="../common/XSD1_1TestCategories.xml#xsd1_1-ID-IDREF-MultipleAttrsOfTypeID"/>
        <schemaTest name="id001.xsd">
            <schemaDocument xlink:href="../saxonData/Id/id001.xsd"/>
            <expected validity="valid"/>
            <current status="accepted" date="2010-05-12"/>
        </schemaTest>
        <instanceTest name="id001.v01.xml">
		        <annotation>
		            <documentation>
		                Valid, two IDs on the same element can have the same value.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Id/id001.v01.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2010-05-12"/>
        </instanceTest>        
        <instanceTest name="id001.n01.xml">
