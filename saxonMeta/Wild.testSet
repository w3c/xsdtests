      version="1.1"
      xmlns:saxon="http://saxon.sf.net/"
      contributor="Saxonica">
      
    <annotation>
            <documentation>
                <Title>Tests for XSD 1.1 wildcard extensions</Title>
                <Description>A number of tests, both valid and invalid, demonstrating use of the new
                XSD 1.1 features for wildcards, for example the notNamespace and notQName attributes.</Description>
            </documentation>
    </annotation>  

    <testGroup version="1.1" name="wild001">
        <annotation>
            <documentation>
                <Title>Basic test of xs:anyAttribute with notNamespace attribute</Title>
                <Description>Allows any attribute except for two designated namespaces</Description>
            </documentation>
        </annotation>
        <documentationReference xlink:href="http://www.w3.org/TR/2009/WD-xmlschema11-1-20091203/structures.html#Wildcards"/>
        <documentationReference xlink:href="../common/XSD1_1TestCategories.xml#xsd1_1-Wildcards-NotNS"/>
        <schemaTest name="wild001.xsd">
            <schemaDocument xlink:href="../saxonData/Wild/wild001.xsd"/>
            <expected validity="valid"/>
            <current status="accepted" date="2008-10-17"/>
        </schemaTest>
        <instanceTest name="wild001.v1.xml">
		        <annotation>
		            <documentation>
		                Valid, no attributes present.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Wild/wild001.v1.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2008-10-17"/>
        </instanceTest>        
        <instanceTest name="wild001.v2.xml">
		        <annotation>
		            <documentation>
		                Valid, permitted attributes present.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Wild/wild001.v2.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2008-10-17"/>
        </instanceTest>                
        <instanceTest name="wild001.n1.xml">
		        <annotation>
		            <documentation>
		                Invalid, disallowed attributes present.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Wild/wild001.n1.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2008-10-17"/>
        </instanceTest>         
        <instanceTest name="wild001.n2.xml">
		        <annotation>
		            <documentation>
		                Invalid, disallowed attributes present.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Wild/wild001.n2.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2008-10-17"/>
        </instanceTest>                 
    </testGroup>
    
