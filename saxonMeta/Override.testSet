    <testGroup name="over001" version="1.1">
        <annotation>
            <documentation>
                <Title>xs:override overriding an element declaration</Title>
                <Description>xs:override overriding an element declaration</Description>
            </documentation>
        </annotation>
        <documentationReference xlink:href="http://www.w3.org/TR/xmlschema11-1/#override-schema"/>
        <documentationReference xlink:href="../common/XSD1_1TestCategories.xml#xsd1_1-SchemaComposition-Override"/>
        <schemaTest name="over001.xsd">
            <schemaDocument xlink:href="../saxonData/Override/over001.xsd"/>
            <expected validity="valid"/>
            <current status="accepted" date="2010-05-05"/>
        </schemaTest>
        <instanceTest name="over001.v01.xml">
		        <annotation>
		            <documentation>
		                Valid, satisfies the overriding declaration.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Override/over001.v01.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2010-05-05"/>
        </instanceTest>        
        <instanceTest name="over001.n01.xml">
		        <annotation>
		            <documentation>
		                Invalid, uses content model before override.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Override/over001.n01.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2010-05-05"/>
        </instanceTest>        
        <instanceTest name="over001.n02.xml">
		        <annotation>
		            <documentation>
		                Invalid, unknown element declaration.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/Override/over001.n02.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2010-05-05"/>
        </instanceTest>                               
    </testGroup>
    
