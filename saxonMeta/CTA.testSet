    <testGroup name="cta0001">
        <annotation>
            <documentation>
                <Title>Conditional simple type: example based on spec</Title>
                <Description>Simple type of message depends on enumerated kind attribute</Description>
            </documentation>
        </annotation>
        <documentationReference xlink:href="http://www.w3.org/TR/xmlschema11-1/#cTypeAlternative"/>
        <documentationReference xlink:href="../common/XSD1_1TestCategories.xml#xsd1_1-CTA-Basic"/>
        <schemaTest name="cta0001">
            <schemaDocument xlink:href="../saxonData/CTA/cta0001.xsd"/>
            <expected validity="valid"/>
            <current status="accepted" date="2007-11-12"/>
        </schemaTest>
        <instanceTest name="cta0001.v01">
		        <annotation>
		            <documentation>
		                Valid, message with kind="date".
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/CTA/cta0001.v01.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2007-11-12"/>
        </instanceTest>        
        <instanceTest name="cta0001.v02">
		        <annotation>
		            <documentation>
		                Valid, message with kind="time".
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/CTA/cta0001.v02.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2007-11-12"/>
        </instanceTest>
        <instanceTest name="cta0001.v03">
		        <annotation>
		            <documentation>
		                Valid, multiple messages at inner level.
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/CTA/cta0001.v03.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2007-11-12"/>
         </instanceTest>                                 
         <instanceTest name="cta0001.n01">
		        <annotation>
		            <documentation>
		                Invalid, message with kind="date" but content is a time
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/CTA/cta0001.n01.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2007-11-12"/>
         </instanceTest>        
         <instanceTest name="cta0001.n02">
		        <annotation>
		            <documentation>
		                Invalid, message with kind="date" at inner level
		            </documentation>
		        </annotation>
            <instanceDocument xlink:href="../saxonData/CTA/cta0001.n02.xml"/>
            <expected validity="invalid"/>
            <current status="accepted" date="2007-11-12"/>
