            <ts:instanceDocument xlink:href="../ibmData/valid/S3_12/s3_12v04.xml"/>
            <ts:expected validity="valid"/>
            <ts:current date="2010-12-01" status="accepted"/>
        </ts:instanceTest>
    </ts:testGroup>

