        <ts:schemaTest name="s3_12v01s">
            <ts:schemaDocument xlink:href="../ibmData/valid/S3_12/s3_12v01.xsd"/>
            <ts:expected validity="valid"/>
            <ts:current date="2010-12-01" status="accepted"/>
        </ts:schemaTest>
        <ts:instanceTest name="s3_12v01i">
            <ts:instanceDocument xlink:href="../ibmData/valid/S3_12/s3_12v01.xml"/>
