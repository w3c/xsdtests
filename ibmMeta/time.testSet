    <ts:testGroup name="d3_3_9v01" version="1.1">
        <ts:annotation>
            <ts:documentation>
				1a: chameleon include on unqualified names in XPath expressions
		        1b: A calendar day with a very early timezone may be completely disjoint from a calendar day with a very late timezone.
		        1c: A time in a timezone may convert to a UTC time on a different day. 
		        </ts:documentation>
        </ts:annotation>
        <ts:documentationReference xlink:href="http://www.w3.org/TR/xmlschema11-2/#time"/>
        <ts:documentationReference xlink:href="../common/XSD1_1TestCategories.xml#xsd1_1-DateTimeTypes-ExplicitTZFacet"/>
        <ts:schemaTest name="d3_3_9v01s">
            <ts:schemaDocument xlink:href="../ibmData/valid/D3_3_9/d3_3_9v01.xsd"/>
            <ts:expected validity="valid"/>
            <ts:current date="2010-12-01" status="accepted"/>
        </ts:schemaTest>
