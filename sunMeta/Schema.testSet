    <testGroup name="annotations00101m1">
        <annotation>
            <documentation>
                <Title>human-targeted  annotation for the schema itself (valid schema)</Title>
                <Description>Annotations provide for human- and machine-targeted 
                             annotations of schema components.
                              In the test the human-targeted annotation 
                             is provided for the schema itself.</Description>
            </documentation>
        </annotation>
        <documentationReference xlink:href="http://www.w3.org/TR/2001/REC-xmlschema-1-20010502/#Schemas"/>
        <schemaTest name="annotations00101m1">
            <schemaDocument xlink:href="../sunData/Schema/annotations/annotations00101m/annotations00101m1.xsd"/>
            <expected validity="valid"/>
            <current status="accepted" date="2005-06-21"/>
        </schemaTest>
        <instanceTest name="Positive">
            <instanceDocument xlink:href="../sunData/Schema/annotations/annotations00101m/annotations00101m1_p.xml"/>
            <expected validity="valid"/>
            <current status="accepted" date="2005-06-21"/>
